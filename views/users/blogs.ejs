<!DOCTYPE html>
<html lang="en">
<head>
     <%- include("../partials/head")  %> 
    
</head>
<body>
    

   <%- include('../partials/nav') %>

    <div class="container mt-3">
        <div class="row">
            <div class="col-md-9">
                <h1 class="h3 mb-3" style="font-weight: bold; font-family: Cambria, Cochin, Georgia, Times, 'Times New Roman', serif;"><%=kategori.name %></h1>
                <div class="row">
                    <% blogs.forEach(a => { %>
                        <div class="col-md-6 mb-5">
                            <div class="card">
                                <img src="/static/images/<%= a.resim %>" class="card-img-top" alt="...">
                                <div class="card-body">
                                    <h5 class="card-title"><%= a.name %></h5>
                                    <p class="card-text"><%- a.aciklama %></p>
                                </div>
                            </div>
                        </div>
                    <% }) %>
                </div>
            </div>
        </div>
    </div>

</body>
</html>